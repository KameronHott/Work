@if (Request.IsSecureConnection) {
    Page.UrlPrefix = "https";
} else {
    Page.UrlPrefix = "http";
}

<!DOCTYPE html>
<html>
<head>
    @*<meta charset="utf-8" />*@
    <title>@ViewBag.Title</title>
    <link href="@Page.UrlPrefix://scripts.cdn.yrmc.org/js/jquery-1.8/css/ui-lightness/jquery-ui-1.8.13.custom.css" rel="stylesheet" type="text/css" />
    <script src="@Page.UrlPrefix://scripts.cdn.yrmc.org/js/jquery-1.8/js/jquery-1.5.1.min.js" type="text/javascript" />

    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    @RenderSection( "TitleContent", false)
    @foreach (string style in ViewBag.StyleSheets) { 
        <link href="@Url.Content(style)" type="text/css" rel="Stylesheet" />
    }
</head>
<body>
    <div class="page">
        <div id="main">
            @RenderBody()
        </div>
        <div id="footer">
            <table>
                <tr>
                    <td>Nursing Bed Board v2.1.0</td>
                </tr>
            </table>
        </div>
    </div>
</body>
</html>
