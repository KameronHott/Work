<!DOCTYPE html>
<!--<html debug="true">-->
<html>
<head>
    <meta http-equiv="Cache-Control" content="no-cache" />
    <meta http-equiv="Pragma" content="no-cache" />
    <meta http-equiv="Expires" content="-1" />
    <title>@ViewBag.Title</title>
    <!--<script src="https://cdn.yrmc.org/js/firebug-lite/build/firebug-lite.js" type="text/javascript"></script>-->
    <link href="https://cdn.yrmc.org/js/jquery-1.8/css/ui-lightness/jquery-ui-1.8.16.custom.css" rel="stylesheet" type="text/css" />
    <script src="https://cdn.yrmc.org/js/jquery-1.8/js/jquery-1.7.1.min.js" type="text/javascript"></script>
    @foreach (string style in ViewBag.StyleSheets)
    { <link href="@Url.Content(style)" type="text/css" rel="Stylesheet" /> }
    @RenderSection("TitleContent", false)
</head>

<body>
    @RenderBody()
</body>
</html>
