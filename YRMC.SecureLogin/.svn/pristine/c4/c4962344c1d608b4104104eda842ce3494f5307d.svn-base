<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Cache-Control" content="no-cache" />
    <meta http-equiv="Pragma" content="no-cache" />
    <meta http-equiv="Expires" content="-1" />
    <title>@ViewBag.Title</title>
    <link href="http://cdn.yrmc.org/js/jquery-1.8/css/ui-lightness/jquery-ui-1.8.16.custom.css" rel="stylesheet" type="text/css" />
    <link href= '@Url.Content("~/Content/Site.css")' rel="stylesheet" type="text/css" />
    <script src="http://cdn.yrmc.org/js/jquery-1.8/js/jquery-1.5.1.min.js" type="text/javascript"></script>
    @foreach (string style in ViewBag.StyleSheets)
    { <link href="@Url.Content(style)" type="text/css" rel="Stylesheet" /> }
    @RenderSection("TitleContent", false)
</head>

<body>
    @RenderBody()
</body>
</html>
